{
  "name": "dsa-slackbot",
  "version": "1.0.0",
  "main": "./dist/bot.js",
  "scripts": {
    "build": "tsc && npm run copy",
    "copy": "npm run copy:package & npm run copy:site",
    "copy:package": "copyfiles package.json package-lock.json dist/",
    "copy:site": "copyfiles src/public/**/* src/sass/**/* dist/ --up 1",
    "prestart": "npm run copy",
    "start:dev": "tsc-watch --onSuccess \"node dist/bot.js\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "botbuilder-adapter-web": "^1.0.7",
    "botbuilder-storage-mongodb": "^0.9.5",
    "botkit": "^4.6.2",
    "botkit-plugin-cms": "^1.0.3",
    "chrono-node": "^1.4.4",
    "copyfiles": "^2.2.0",
    "dotenv": "^7.0.0",
    "googleapis": "^48.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
